<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
       http://www.springframework.org/schema/beans/spring-beans-3.2.xsd  
       http://www.springframework.org/schema/context  
       http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<bean id="huanXinUtil"
		class="com.sifude.lejane.util.HuanXinUtil" >
		<property name="clientId" value="${huanxin.client.id}"></property>
		<property name="clientSecret" value="${huanxin.client.secret}"/>
		<property name="urlToken" value="${huanxin.url.token}"/>
		<property name="urlRegist" value="${huanxin.url.regist}"/>
		<property name="urlBatchRegist" value="${huanxin.url.batchregist}"/>
		<property name="urlChat" value="${huanxin.url.chat}"/>
	</bean>
	
	
	
</beans> 